{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Maven: Clean Install",
			"type": "shell",
			"command": "./mvnw",
			"args": [
				"clean",
				"install"
			],
			"group": "build",
			"problemMatcher": [],
			"isBackground": false
		},
		{
			"label": "Spring Boot: Run",
			"type": "shell",
			"command": "./mvnw",
			"args": [
				"spring-boot:run"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"isBackground": true,
			"problemMatcher": []
		},
		{
			"label": "Maven: Test",
			"type": "shell",
			"command": "./mvnw",
			"args": [
				"test"
			],
			"group": "test",
			"problemMatcher": [],
			"isBackground": false
		},
		{
			"label": "Maven: Compile",
			"type": "shell",
			"command": "./mvnw",
			"args": [
				"compile"
			],
			"group": "build",
			"problemMatcher": [],
			"isBackground": false
		}
	]
}